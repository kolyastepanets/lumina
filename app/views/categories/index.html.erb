<div class='container'>
  <div class='row links-to-category'>
    <% @categories.each do |category| %>
      <div class='col-xs-3'>
        <a href='#' class='category-link'>
          <%= category.title %>
        </a>
      </div>
    <% end %>
  </div>
  <div class='row'>
    <% @albulms.each do |albulm| %>
      <div class='col-md-2 col-sm-4 col-xs-6 albulm-wrap'>
        <%= link_to albulm_path(albulm) do %>
          <%= image_tag albulm.title_photo %>
          <div class='albulm-title-wrap non-visibility'>
            <h3>
              <span><%= albulm.title %></span>
            </h3>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
